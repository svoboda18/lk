  &DISP_MUTEX0_RST=&disp_mutex_base+0x28
  &DISP_MUTEX1_SOF=&disp_mutex_base+0x50
  &DISP_MUTEX1_MOD=&disp_mutex_base+0x4c
  &DISP_MUTEX1_EN=&disp_mutex_base+0x40
  &DISP_MUTEX1_RST=&disp_mutex_base+0x48
  
